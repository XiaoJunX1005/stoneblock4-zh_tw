	quest.4498E3F4B2DC24FC.title: "&a寶石"
	quest.44A61436D2FE34E9.quest_subtitle: "鐵雞"
	quest.44A61436D2FE34E9.title: "一個好開始"
	quest.44B2831C7225AB4B.title: "能量存儲部件"
